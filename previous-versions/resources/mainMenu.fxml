<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PresentationState.MainMenu.MainMenuController">
   <children>
      <Button fx:id="appointmentsBtn" mnemonicParsing="false" styleClass="mainMenuBtn" text="Appointments" />
      <Button fx:id="customersBtn" layoutX="10.0" layoutY="28.0" mnemonicParsing="false" styleClass="mainMenuBtn" text="Customers" />
      <Button fx:id="consultantsBtn" layoutX="10.0" layoutY="28.0" mnemonicParsing="false" styleClass="mainMenuBtn" text="Consultants" />
      <Button fx:id="myMonthBtn" layoutX="10.0" layoutY="53.0" mnemonicParsing="false" styleClass="mainMenuBtn" text="My Month" />
      <Button fx:id="myWeekBtn" layoutX="10.0" layoutY="79.0" mnemonicParsing="false" styleClass="mainMenuBtn" text="My Week" />
      <Button fx:id="reportsBtn" layoutX="10.0" layoutY="104.0" mnemonicParsing="false" styleClass="mainMenuBtn" text="Reports" />
      <VBox prefHeight="0.0" prefWidth="100.0">
         <children>
            <Label id="nextAppointmentHeader" fx:id="nextAppointmentHeader" text="Next Appointment" />
            <Label fx:id="dateLabel" styleClass="nextAppointmentLabel" text="Date" />
            <Label fx:id="timeLabel" layoutX="10.0" layoutY="28.0" text="Time" />
            <Label fx:id="customerNameLabel" layoutX="10.0" layoutY="45.0" text="Customer Name" />
            <Label fx:id="customerPhoneLabel" layoutX="10.0" layoutY="63.0" text="Phone" />
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets bottom="25.0" left="20.0" right="20.0" top="25.0" />
   </padding>
</VBox>
